{
  "name": "ts-move-helper",
  "displayName": "TS Move Helper",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/Extension/Extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tsMoveHelper.move",
        "title": "Move TypeScript File/Dir"
      },
      {
        "command": "tsMoveHelper.acceptQuickPickSuggestion",
        "title": "Accept Quick Pick Suggestion"
      }
    ],
    "keybindings": [
      {
        "key": "tab",
        "command": "tsMoveHelper.acceptQuickPickSuggestion",
        "when": "quickInputVisible"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "tsMoveHelper.move",
          "when": "explorerResourceIsFolder == true",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "compile_watch": "tsc -watch -p ./",
    "testCompile": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest",
    "type_check": "npx tsc --noEmit --watch"
  },
  "dependencies": {
    "callsites": "^4.2.0",
    "cli-progress": "^3.12.0",
    "fs-extra": "^11.2.0",
    "jscodeshift": "^17.3.0",
    "react": "18.3.1",
    "ts-morph": "^22.0.0",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jscodeshift": "^17.3.0",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
